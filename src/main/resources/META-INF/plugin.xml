<idea-plugin>
  <id>com.intelij.idea.xtext</id>
  <name>Xtext</name>
  <version>1.0</version>
  <vendor email="" url="">JetBrains</vendor>

  <!--  <description><![CDATA[-->
  <!--      Enter short description for your plugin here.<br>-->
  <!--      <em>most HTML tagds may be used</em>-->
  <!--    ]]></description>-->

  <!--  <change-notes><![CDATA[-->
