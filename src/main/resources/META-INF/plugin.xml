  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">

    <fileTypeFactory implementation="com.intellij.xtextLanguage.xtext.XtextFileTypeFactory" />
    <lang.parserDefinition language="Xtext" implementationClass="com.intellij.xtextLanguage.xtext.XtextParserDefinition"/>

    <lang.syntaxHighlighterFactory language="Xtext" implementationClass="com.intellij.xtextLanguage.xtext.XtextSyntaxHighlighterFactory"/>
      <psi.referenceContributor language="Xtext"
                                implementation="com.intellij.xtextLanguage.xtext.XtextReferenceContributor"/>
      <braceMatcher filetype="Xtext" implementationClass="com.intellij.xtextLanguage.xtext.XtextBraceMatcher"/>
      <lang.braceMatcher language="Xtext"
                         implementationClass="com.intellij.xtextLanguage.xtext.XtextPairedBraceMatcher"/>
      <completion.contributor language="Xtext"
                              implementationClass="com.intellij.xtextLanguage.xtext.XtextCompletionContributor"/>


    <!--    <fileTypeFactory implementation="com.intellij.exprLanguage.expr.exprFileTypeFactory" />-->
    <!--    <lang.parserDefinition language="expr" implementationClass="com.intellij.exprLanguage.expr.exprParserDefinition"/>-->

    <!--    <lang.syntaxHighlighterFactory language="expr" implementationClass="com.intellij.exprLanguage.expr.exprSyntaxHighlighterFactory"/>-->
    <!--    <psi.referenceContributor language="expr"-->
    <!--                              implementation="com.intellij.exprLanguage.expr.exprReferenceContributor"/>-->

    <!--    <completion.contributor language="expr"-->
    <!--                            implementationClass="com.intellij.exprLanguage.expr.exprCompletionContributor"/>-->
    <!--    <fileTypeFactory implementation="com.intellij.entityLanguage.entity.EntityFileTypeFactory"/>-->
    <!--    <lang.parserDefinition language="Entity"-->
    <!--                           implementationClass="com.intellij.entityLanguage.entity.EntityParserDefinition"/>-->
    <!--    <lang.syntaxHighlighterFactory language="Entity"-->
    <!--                                   implementationClass="com.intellij.entityLanguage.entity.EntitySyntaxHighlighterFactory"/>-->
    <!--    <completion.contributor language="Entity"-->
    <!--                            implementationClass="com.intellij.entityLanguage.entity.EntityCompletionContributor"/>-->
    <!--    <psi.referenceContributor language="Entity"-->
    <!--                              implementation="com.intellij.entityLanguage.entity.EntityReferenceContributor"/>-->

    <fileTypeFactory implementation="com.intellij.smalljavaLanguage.smalljava.SmalljavaFileTypeFactory"/>
    <lang.parserDefinition language="Smalljava"
                           implementationClass="com.intellij.smalljavaLanguage.smalljava.SmalljavaParserDefinition"/>
    <lang.syntaxHighlighterFactory language="Smalljava"
                                   implementationClass="com.intellij.smalljavaLanguage.smalljava.SmalljavaSyntaxHighlighterFactory"/>
    <completion.contributor language="Smalljava"
                            implementationClass="com.intellij.smalljavaLanguage.smalljava.SmalljavaCompletionContributor"/>
    <psi.referenceContributor language="Smalljava"
                              implementation="com.intellij.smalljavaLanguage.smalljava.SmalljavaReferenceContributor"/>
    <!--    <lang.findUsagesProvider language="Smalljava"-->
    <!--                             implementationClass="com.intellij.smalljavaLanguage.smalljava.SmalljavaFindUsagesProvider"/>-->

    <fileTypeFactory implementation="com.intellij.calc2Language.calc2.Calc2FileTypeFactory"/>
    <lang.parserDefinition language="Calc2"
                           implementationClass="com.intellij.calc2Language.calc2.Calc2ParserDefinition"/>
    <lang.syntaxHighlighterFactory language="Calc2"
                                   implementationClass="com.intellij.calc2Language.calc2.Calc2SyntaxHighlighterFactory"/>
    <completion.contributor language="Calc2"
                            implementationClass="com.intellij.calc2Language.calc2.Calc2CompletionContributor"/>
    <psi.referenceContributor language="Calc2"
                              implementation="com.intellij.calc2Language.calc2.Calc2ReferenceContributor"/>
    <lang.refactoringSupport language="Calc2"
                             implementationClass="com.intellij.calc2Language.calc2.Calc2RefactoringSupportProvider"/>


    <applicationService serviceInterface="com.intellij.xtextLanguage.xtext.EcorePackageRegistry"
                        serviceImplementation="com.intellij.xtextLanguage.xtext.EcorePackageRegistry$ListBasedRegistry"/>
  </extensions>


</idea-plugin>