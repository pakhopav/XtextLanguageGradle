<extensions defaultExtensionNs="com.intellij">
    <fileTypeFactory implementation="com.intellij.statLanguage.stat.StatFileTypeFactory"/>
    <lang.parserDefinition language="Stat" implementationClass="com.intellij.statLanguage.stat.StatParserDefinition"/>
    <lang.syntaxHighlighterFactory language="Stat"
                                   implementationClass="com.intellij.statLanguage.stat.StatSyntaxHighlighterFactory"/>
    <completion.contributor language="Stat"
                            implementationClass="com.intellij.statLanguage.stat.StatCompletionContributor"/>
    <psi.referenceContributor language="Stat" implementation="com.intellij.statLanguage.stat.StatReferenceContributor"/>

</extensions>