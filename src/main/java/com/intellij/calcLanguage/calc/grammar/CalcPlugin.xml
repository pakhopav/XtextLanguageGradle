    <lang.parserDefinition language="Calc" implementationClass="com.intellij.calcLanguage.calc.CalcParserDefinition"/>
    <lang.syntaxHighlighterFactory language="Calc"
                                   implementationClass="com.intellij.calcLanguage.calc.CalcSyntaxHighlighterFactory"/>
    <completion.contributor language="Calc"
                            implementationClass="com.intellij.calcLanguage.calc.CalcCompletionContributor"/>
    <psi.referenceContributor language="Calc" implementation="com.intellij.calcLanguage.calc.CalcReferenceContributor"/>

</extensions>